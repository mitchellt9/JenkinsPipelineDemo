pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'I am  building'
            }
        }
    
    
        stage('Deploy') {
            steps {
                echo 'I am deploying'
            }
        }
    
    
        stage('Test') {
            steps {
                echo 'I am testing'
            }
        }
    }
}
